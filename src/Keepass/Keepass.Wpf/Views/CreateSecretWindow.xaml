<Window x:Class="Keepass.Wpf.Views.CreateSecretWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Keepass.Wpf.Views"
        mc:Ignorable="d"
        Title="CreateSecretWindow" Height="550" Width="350">
    <Grid>
        <StackPanel Orientation="Vertical">
            <StackPanel Orientation="Horizontal" Margin="20 30 20 10">
                <Label Content="Username"/>
                <TextBox x:Name="textBoxUsername" Padding="100 3" />
            </StackPanel>
            <StackPanel Orientation="Horizontal" Margin="20 10 20 10">
                <Label Content="Password"/>
                <TextBox x:Name="textBoxPassword" Padding="100 3" />
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
