<Window x:Class="Keepass.Wpf.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Keepass.Wpf.Views"
        mc:Ignorable="d"
        Title="Keepass" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Margin="0,0,0,355">
            <StackPanel.Resources>
                <Style TargetType="{x:Type Button}">
                    <Setter Property="Padding" Value="15,0"/>
                    <Setter Property="Margin" Value="5,20"/>
                </Style>
            </StackPanel.Resources>
            <Button x:Name="btnCreateSecrets" Content="N" Click="btnCreateSecrets_Click" />
        </StackPanel>
        <StackPanel>
            <DataGrid x:Name="dataGridSecrets" AutoGenerateColumns="True" Margin="0,75" />
        </StackPanel>
    </Grid>
</Window>
