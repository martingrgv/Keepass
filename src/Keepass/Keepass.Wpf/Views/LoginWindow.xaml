<Window x:Class="Keepass.Wpf.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Keepass.Wpf"
        mc:Ignorable="d"
        Title="Login" Height="280" Width="400">
    <Grid>
        <StackPanel Orientation="Vertical">
            <StackPanel Orientation="Horizontal">
                <Label x:Name="labelKey" Content="Key:"/>
                <TextBlock x:Name="textBlockKey" />
            </StackPanel>

            <Button x:Name="btnLogin" Content="Log In" Margin="120 30"/>
        </StackPanel>
    </Grid>
</Window>
